{
  "name": "anni-nerchukundam",
  "version": "1.0.0",
  "description": "A fullstack starter template with Next.js, Express, Knex.js with Typescript preconfigured",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all clean --parallel client:dev server:dev",
    "build": "npm-run-all clean --parallel client:build server:build",
    "start": "npm-run-all build --parallel client:start server:start",
    "client:dev": "npm-run-all wait:server client:watch",
    "client:build": "next build",
    "client:start": "npm-run-all wait:server client:up",
    "server:dev": "npm-run-all server:build --parallel server:build:watch server:nodemon",
    "server:build": "npm-run-all --parallel shared:babel server:babel",
    "server:start": "node server/dist/app.js",
    "server:build:watch": "npm-run-all --parallel \"shared:babel -- --watch\" \"server:babel -- --watch\"",
    "server:babel": "babel --config-file ./server/babel.config.json --out-dir ./server/dist ./server --extensions \".ts,.js\"",
    "server:nodemon": "nodemon server/dist/app.js",
    "shared:babel": "babel --out-dir ./server/dist/shared ./shared --extensions \".ts,.js\"",
    "wait:server": "wait-on http://localhost:4000",
    "client:up": "next start",
    "client:watch": "next dev",
    "clean": "rimraf .next server/dist",
    "test": "jest --watch",
    "test:run": "jest",
    "test:coverage": "jest --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "wait-on http://localhost:3000 && npm run cy:run",
    "check:types": "npm-run-all --parallel check:types:client check:types:server check:types:cypress",
    "check:types:client": "tsc --noEmit",
    "check:types:server": "tsc --noEmit --p server/tsconfig.json",
    "check:types:cypress": "tsc --noEmit --p cypress/tsconfig.json",
    "lint-staged": "lint-staged",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:validate": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "postinstall": "husky install",
    "validate": "npm-run-all prettier:validate check:types lint test:coverage test:e2e:run",
    "migrate:make": "knex migrate:make --knexfile ./server/knex/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile ./server/knex/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./server/knex/knexfile.js",
    "migrate:up": "knex migrate:up --knexfile ./server/knex/knexfile.js",
    "migrate:down": "knex migrate:down --knexfile ./server/knex/knexfile.js",
    "migrate:unlock": "knex migrate:unlock --knexfile ./server/knex/knexfile.js",
    "seed:make": "knex seed:make --knexfile ./server/knex/knexfile.js",
    "seed:run": "knex seed:run --knexfile ./server/knex/knexfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvgdev/anni-nerchukundam.git"
  },
  "keywords": [
    "template",
    "next.js",
    "knex",
    "postgresql",
    "express",
    "typescript",
    "jest",
    "cypress"
  ],
  "author": "pvgdev <pvgdev@outlook.com> (https://vishu.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pvgdev/anni-nerchukundam/issues"
  },
  "homepage": "https://github.com/pvgdev/anni-nerchukundam#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/jest": "^11.1.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.3",
    "cypress": "^6.4.0",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "jest-environment-node": "^26.6.2",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.12.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/react": "^11.1.4",
    "apollo-server": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "next": "^10.0.6",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
